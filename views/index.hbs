<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>SEpro3</title>

  <!-- Bootstrap core CSS -->
  <link href="/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

  <!-- Custom styles for this template -->
  <link href="/css/scrolling-nav.css" rel="stylesheet">
 <!-- Step 1 - Including the fusioncharts core library -->
  <script type="text/javascript" src="https://cdn.fusioncharts.com/fusioncharts/latest/fusioncharts.js"></script>
  <!-- Step 2 - Including the fusion theme -->
  <script type="text/javascript" src="https://cdn.fusioncharts.com/fusioncharts/latest/themes/fusioncharts.theme.fusion.js"></script>
</head>

<body id="page-top">
  <section class="callout" style="padding: 30px auto; ">
    <div class="container text-left">
      <div class="row">
        {{!-- Chart starts --}}
        <div class="col-md-8">
          <div id="chart-container">Fusion Chart Here</div>
        </div>
        {{!-- chart ends --}}
        <div class="col-md-2" style="padding:auto 50px">
          <div class="card" style="width: 18rem;border:5px solid black">
            <div class="card-body">
              <h5 class="card-title">Humidity</h5>
              <h6 class="card-subtitle mb-2 text-muted"><small>g/m3</small></h6>
              <p class="card-text text-center" id="gas-conc" style="font-size:50px">
                {{#if result.humidity}}{{result.humidity}}{{else}}--{{/if}}
              </p>

              <small class="float-right">time: <span class="t_stamp">{{#if
                  result.time_stamp}}{{result.time_stamp}}{{else}}--{{/if}}</span></small>
            </div>
          </div>
          <br>
          <div class="card" style="width: 18rem;border:5px solid black">
            <div class="card-body">
              <h5 class="card-title">Temperature</h5>
              <h6 class="card-subtitle mb-2 text-muted"><sup>o</sup>C</h6>
              <p class="card-text text-center" id="temperature" style="font-size: 50px">
                {{#if result.temperature}}{{result.temperature}}{{else}}--{{/if}}
              </p>

              <small class="float-right">time: <span class="t_stamp">{{#if
                  result.time_stamp}}{{result.time_stamp}}{{else}}--{{/if}}</span></small>
            </div>
          </div>
          <br>

        </div>
      </div>
    </div>
  </section>

  <!-- Map -->
  <section id="contact" class="map">
    <iframe width="100%" height="100%" src="https://maps.google.com/maps?width=100%&amp;height=600&amp;hl=en&amp;coord=5.7597712,-0.2219285&amp;q=1%20University%20Avenue%2C%20%20Berekuso+(Ashesi%20University)&amp;ie=UTF8&amp;t=&amp;z=14&amp;iwloc=B&amp;output=embed"
      frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
      <a href="https://www.maps.ie/map-my-route/">Draw map route</a>
    </iframe>
  </section>
  <script type="text/javascript" src="/js/chart.js"></script>
</body>

</html>
